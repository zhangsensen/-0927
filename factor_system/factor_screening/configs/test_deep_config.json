{
  "name": "deep",
  "description": "深度筛选配置 - 全面的因子分析",
  "data_root": "./output",
  "raw_data_root": "../raw",
  "symbols": [
    "0700.HK"
  ],
  "timeframes": [
    "60min"
  ],
  "ic_horizons": [
    1,
    3,
    5,
    10,
    20,
    30
  ],
  "min_sample_size": 200,
  "alpha_level": 0.05,
  "fdr_method": "benjamini_hochberg",
  "rolling_window": 120,
  "min_ic_threshold": 0.01,
  "min_ir_threshold": 0.25,
  "min_robustness_score": 0.6,
  "vif_threshold": 3.0,
  "correlation_threshold": 0.7,
  "base_factors": [
    "MA5",
    "MA10",
    "RSI14",
    "MACD_12_26_9"
  ],
  "commission_rate": 0.002,
  "slippage_bps": 5.0,
  "market_impact_coeff": 0.1,
  "min_stability_threshold": 0.6,
  "max_vif_threshold": 10.0,
  "max_cost_threshold": 0.01,
  "weights": {
    "predictive_power": 0.35,
    "stability": 0.25,
    "independence": 0.2,
    "practicality": 0.1,
    "short_term_fitness": 0.1
  },
  "max_workers": 6,
  "enable_parallel": true,
  "output_dir": "./output",
  "save_reports": true,
  "save_detailed_metrics": true,
  "log_level": "INFO",
  "memory_limit_gb": 8.0,
  "timeout_minutes": 60
}